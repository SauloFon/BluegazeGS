@{
    ViewData["Title"] = "Criar Usuarios";
}
<div class="text-center">
    <h1 class="display-4">Criar Usuario</h1>
<div/>
    
    <form>
        <div class="form-group">
            <label for="username">Username</label>
            <input type="text" class="form-control" placeholder="Digite seu username">
        </div>
        <div class="form-group">
            <label for="email">E-mail</label>
            <input type="text" class="form-control" placeholder="Digite seu E-mail">
        </div>
        <div class="form-group">
            <label for="Criado em">Criado em</label>
            <input type="text" class="form-control" placeholder="Data de criação">
        </div>
        <button type="submit" class="btn btn-primary" btn-block>Adicionar</button>
        
        <a class="btn btn-secondary" asp-controller="Usuarios" asp-action="Index" role="button" btn-block>Voltar</a>
        
    </form>

